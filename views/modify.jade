extends layout

block subhead
  script(src="javascripts/jquery-2.1.1.js")
  script(src="javascripts/versioner.js")
  script(src="javascripts/tabler.js")
  script(src="javascripts/newrow.js")

block content
  div(id="interface", align="middle")
    h2 Load autoPACK Table &nbsp
    select(name='recname', class='recn')
      each rn, index in recNames
        option(value=rn)= rn
    select(name='recversion', class="recv")
    button(id="loader", type="button") Load Table
  div(id="tableDiv", align="middle")
    form(id="tableForm", action="/commit", method="post")
      br
      div(align="middle")
        button(type="submit", id="commitButton") Commit Changes
      br
      br